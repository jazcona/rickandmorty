{
  "name": "rickandmorty-app",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "tauri:build:windows": "tauri build --target x86_64-pc-windows-msvc",
    "tauri:build:macos": "tauri build --target x86_64-apple-darwin",
    "tauri:build:macos-arm": "tauri build --target aarch64-apple-darwin",
    "tauri:build:all": "npm run tauri:build:linux && npm run tauri:build:windows && npm run tauri:build:macos && npm run tauri:build:macos-arm",
    "package": "npm run build && npm run tauri:build"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tauri-apps/cli": "^2.9.2"
  },
  "description": "Una aplicaci√≥n de escritorio para explorar el universo de Rick and Morty usando Tauri",
  "main": "index.js",
  "keywords": [
    "tauri",
    "rick-and-morty",
    "desktop-app"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.48.4",
    "@tauri-apps/api": "^2.9.0",
    "svelte": "^5.43.2",
    "vite": "^7.1.12"
  }
}
